.icon-tuihui:before { content: "\e642"; }

.keyboardBg{position: fixed; left: 0; top: 0; width: 100%; height: 100%; background: rgba(0, 0, 0, 0.4);}
.key_input{position: absolute; width: 100%; left: 0; bottom: 0; }
.keyboards{background: #ebebeb; width: 100%; padding: 0; overflow: hidden; background: #ebebeb;}
.keyboards .keyboard {text-align: center; margin: 15rpx 3rpx 10rpx; display: flex;justify-content: space-around;}
.keyboards .keyboard:first-child {margin-top: 10rpx;}
.keyboards button{text-decoration: none; text-shadow: 0 1px 1px white; border: 0; background: #fff; border-radius: 4px; box-shadow: 0 1px 1px rgba(0, 0, 0, 0.4), 0 1px 0 rgba(0, 0, 0, 0.2); width: 68rpx; margin: 0 6rpx; padding: 2rpx 0; font-size: 40rpx;}
.keyboards button:active {background-image: -webkit-gradient(linear, left top, left bottom, from(#e2e3e4), to(#aaadb4));}
.keyboards .keyboard_switch{width: 100rpx; padding: 11rpx 0; font-size: 34rpx; color: #fff; background: #cccccc;}
.keyboards .keyboard_switch:active{background: #999999;}
.keyboards .keyboard_delete{width: 100rpx; background: #cc3333;font-size: 0;}
.keyboards .keyboard_delete::after{font-family:"iconfont"; content: "\e642";font-size: 80rpx; color: #fff;}
.keyboards .keyboard_delete:active{background: #990000;}
.keyboards .keyboard_delete image{width: 100rpx; height: 104rpx;}
